{
  "name": "frow",
  "version": "3.0.0",
  "description": "Flex Row CSS Grid System http://frowcss.com",
  "main": "_main.sass",
  "scripts": {
    "sass": "node-sass frow.sass dist/frow.css",
    "autoprefixer": "postcss --no-map -u autoprefixer -r dist/frow.css",
    "addnormalize": "FROW=`echo \"/*! Frow CSS with Normalize.css (Vendor Prefixed) */\" | cat - node_modules/normalize.css/normalize.css dist/frow.css` && echo \"$FROW\" > dist/frow.css",
    "cssnano": "postcss --no-map -u cssnano -o dist/frow.min.css dist/frow.css",
    "csscopy": "cp -t docs/styles dist/frow.css",
    "letsgocopy": "cp -t docs/scripts node_modules/letsgo/letsgo.js",
    "build:css": "run-s sass autoprefixer addnormalize cssnano csscopy",
    "build:js": "run-s letsgocopy",
    "build": "run-s build:*",
    "watch:src": "onchange \"frow.sass\" \"_variables.sass\" \"src\" -- run-s build:css",
    "watch": "run-p watch:*",
    "server": "browser-sync start --f \"docs/**/*\" --server \"docs\" --port 8081 --ui-port 8082",
    "running": "run-p watch server",
    "start": "run-s build running"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Beg-in/frow.git"
  },
  "keywords": [
    "css",
    "css grid",
    "responsive css",
    "responsive",
    "flex",
    "css flex",
    "flexbox",
    "sass",
    "mobile-first",
    "front-end",
    "front end",
    "framework",
    "web"
  ],
  "author": "Cody Sherman <cody@beg.in> (http://codysherman.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Beg-in/frow/issues"
  },
  "homepage": "http://frowcss.com",
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "browser-sync": "^2.18.13",
    "cssnano": "^4.0.0-rc.2",
    "letsgo": "^0.2.3",
    "node-sass": "^4.7.1",
    "normalize.css": "*",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.2.1",
    "postcss-cli": "^4.1.1"
  }
}
